<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>wowo</display-name>
  <filter>
    <filter-name>ChartEncoderFilter</filter-name>
    <filter-class>cn.tf.filters.ChartEncoderFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ChartEncoderFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>InitServlet</servlet-name>
    <servlet-class>cn.tf.servlets.InitServlet</servlet-class>
    <init-param>
      <param-name>uploadPath</param-name>
      <param-value>../wowopath</param-value>
    </init-param>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <listener>
    <listener-class>cn.tf.listener.ServletContextListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>RolesServlet</display-name>
    <servlet-name>RolesServlet</servlet-name>
    <servlet-class>cn.tf.servlets.RolesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RolesServlet</servlet-name>
    <url-pattern>/servlet/RolesServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>BasicServlet</servlet-name>
    <servlet-class>cn.tf.servlets.BasicServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BasicServlet</servlet-name>
    <url-pattern>/servlet/BasicServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>adminLogin</display-name>
    <servlet-name>adminLogin</servlet-name>
    <servlet-class>cn.tf.servlets.adminLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>adminLogin</servlet-name>
    <url-pattern>/adminLogin</url-pattern>
  </servlet-mapping>
</web-app>